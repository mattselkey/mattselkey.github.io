<!-- wp:shortcode -->

<figure class="highlight"><pre><code class="language-ruby" data-lang="ruby"><span class="n">git</span> <span class="n">tag</span></code></pre></figure>

<!-- /wp:shortcode -->

<!-- wp:heading -->
<h2>3.2 Delete a Tag</h2>
<!-- /wp:heading -->

<!-- wp:shortcode -->

<figure class="highlight"><pre><code class="language-ruby" data-lang="ruby"><span class="n">git</span> <span class="n">tag</span> <span class="o">-</span><span class="n">d</span> <span class="no">TagName</span></code></pre></figure>

<!-- /wp:shortcode -->

<!-- wp:heading -->
<h2>3.2 Sync Local/Remote Tags</h2>
<!-- /wp:heading -->

<!-- wp:shortcode -->

<figure class="highlight"><pre><code class="language-ruby" data-lang="ruby"><span class="n">git</span> <span class="n">fetch</span> <span class="o">--</span><span class="n">prune</span> <span class="o">--</span><span class="n">tags</span></code></pre></figure>

<!-- /wp:shortcode -->

<!-- wp:heading {"level":1} -->
<h1>4. Subtress</h1>
<!-- /wp:heading -->

<!-- wp:heading -->
<h2>4.1 Add a new Subtree from a remote repository</h2>
<!-- /wp:heading -->

<!-- wp:shortcode -->

<figure class="highlight"><pre><code class="language-ruby" data-lang="ruby"><span class="n">git</span> <span class="n">subtree</span> <span class="n">add</span> <span class="o">--</span><span class="n">prefix</span> <span class="no">LocalFolder</span> <span class="no">RemoteRepoURL</span><span class="p">.</span><span class="nf">git</span> <span class="no">BranchName</span> <span class="o">--</span><span class="n">squash</span></code></pre></figure>

<!-- /wp:shortcode -->

<!-- wp:heading -->
<h2>4.2 Update a Subtree from a remote repository</h2>
<!-- /wp:heading -->

<!-- wp:shortcode -->

<figure class="highlight"><pre><code class="language-ruby" data-lang="ruby"><span class="n">git</span> <span class="n">subtree</span> <span class="n">pull</span> <span class="o">--</span><span class="n">prefix</span> <span class="no">LocalFolder</span> <span class="no">RemoteRepoURL</span><span class="p">.</span><span class="nf">git</span> <span class="no">BranchName</span> <span class="o">--</span><span class="n">squash</span></code></pre></figure>

<!-- /wp:shortcode -->

<!-- wp:heading -->
<h2>4.2 Remove a Subtree</h2>
<!-- /wp:heading -->

<!-- wp:shortcode -->

<figure class="highlight"><pre><code class="language-ruby" data-lang="ruby"><span class="n">git</span> <span class="n">remote</span> <span class="n">rm</span> <span class="no">LocalFolder</span></code></pre></figure>

